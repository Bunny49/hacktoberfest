#include <stdio.h>
int main(void){
int nums[10],num,temp;
for(int i=0;i<10; i++){
    num=rand();
}
printf("Values in the array are \n");
for(int i=0; i<10; i++){
    printf("%d ",nums[i]);
}
    printf("\n");
    for (int i=0;i<10; i++){
            for(int j=i+1;j<10; j++){
                if(nums[i]>nums[j]){
                    int temp=nums[i];
            nums[i]=nums[j];
            nums[j]=temp;

}
}
}
printf("Values After Sorting in Ascending Order\n");
for(int i=0;i<10; i++){
    printf("%d ",nums[i]);
}return 0;
}
